{
    "name": "Pluie dans l'heure",
    "version": "1.4.20150826",
    "manifest_version": 2,
    "description": "Va-t-il pleuvoir dans l'heure ?",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "notifications",
        "http://www.meteofrance.com/"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "icons": {
        "32": "img/rain32.png",
        "128": "img/rain128.png"
    }
}
